#Returns the power of integer x
func power(x:int, power:int)->int{
	
	var i:int=2;
	var result:int;
	result = x;

	while (power >= i){
		result = result * x;
		i = i + 1;
	}

	return result;
}


#Returns the power of real x
func power(x:real, power:int)->real{
	
	var i:int=2;
	var result:real;
	result = x;

	while (power >= i){
		result = result * x;
		i = i + 1;
	}

	return result;
}